{
    "name": "ohdearapp/health-check-report",
    "description": "Create a health check report to be verified by Oh Dear",
    "keywords": [
        "ohdearapp",
        "health-check-report"
    ],
    "homepage": "https://github.com/ohdearapp/health-check-report",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/pest-plugin-snapshots": "^1.1",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "OhDear\\HealthCheckReport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OhDear\\HealthCheckReport\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
